{
  "name": "@avaelements/renderer-web",
  "version": "1.1.0",
  "description": "AvaElements Web Renderer - React components with Flutter Parity + Phase 3 (74 components)",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite",
    "preview": "vite preview",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:a11y": "jest --testPathPattern=a11y",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "bundle-size": "vite-bundle-visualizer",
    "analyze": "vite-bundle-visualizer --open",
    "clean": "rm -rf dist node_modules coverage storybook-static",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "avaelements",
    "react",
    "components",
    "material-ui",
    "flutter-parity",
    "design-system",
    "ui-library"
  ],
  "author": "Manoj Jhawar <manoj@ideahq.net>",
  "license": "Proprietary",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/augmentalis/avanues.git",
    "directory": "Universal/Libraries/AvaElements/Renderers/Web"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@mui/material": "^5.14.0",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0"
  },
  "dependencies": {
    "@mui/icons-material": "^5.14.20",
    "clsx": "^2.0.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "@vitejs/plugin-react": "^4.2.1",
    "vite-bundle-visualizer": "^1.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "jest-axe": "^8.0.0",
    "@storybook/react": "^7.6.4",
    "@storybook/react-vite": "^7.6.4",
    "@storybook/addon-essentials": "^7.6.4",
    "@storybook/addon-interactions": "^7.6.4",
    "@storybook/addon-links": "^7.6.4",
    "@storybook/addon-a11y": "^7.6.4",
    "@storybook/testing-library": "^0.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@mui/material": "^5.14.20",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
