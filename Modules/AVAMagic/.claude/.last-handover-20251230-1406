anager? = null\n    78→) : ViewModel() {\n    79→\n    80→    private val _uiState = MutableStateFlow(HomeUiState())\n    81→\n    82→    /**\n    83→     * Current UI state for the Home screen.\n    84→     *\n    85→     * Emits updates whenever statistics are loaded or an error occurs.\n    86→     * Subscribe to this flow to receive reactive UI updates.\n    87→     */\n    88→    val uiState: StateFlow<HomeUiState> = _uiState.asStateFlow()\n    89→\n    90→    init {\n    91→        // Load statistics when ViewModel is created\n    92→        loadStats()\n    93→        // Load learned apps for clickable display\n    94→        loadLearnedApps()\n    95→    }\n    96→\n    97→    /**\n    98→     * Loads statistics from the database.\n    99→     *\n   100→     * Updates the UI state with:\n   101→     * - Number of learned apps (from scrapedAppCount)\n   102→     * - Number of available commands (from generatedCommandCount)\n   103→     * - Number of commands used today (currently 0, will be implemented with usage tracking)\n   104→     *\n   105→     * Sets isLoading to true while loading and false when complete.\n   106→     * Sets error message if loading fails.\n   107→     */\n   108→    fun loadStats() {\n   109→        viewModelScope.launch {\n   110→            // Set loading state\n   111→            _uiState.update { it.copy(isLoading = true, error = null) }\n   112→\n   113→            try {\n   114→                // Fetch statistics from database\n   115→                val stats = databaseManager.getStats()\n   116→\n   117→                // Update UI state with loaded statistics\n   118→                _uiState.update {\n   119→                    it.copy(\n   120→                        appsLearned = stats.scrapedAppCount,\n   121→                        commandsAvailable = stats.generatedCommandCount,\n   122→                        commandsToday = 0, // TODO: Implement command usage tracking\n   123→                        isLoading = false,\n   124→                        error = null\n   125→                    )\n   126→                }\n   127→            } catch (e: Exception) {\n   128→                // Update UI state with error\n   129→                _uiState.update {\n   130→                    it.copy(\n   131→                        isLoading = false,\n   132→                        error = \"Failed to load statistics: ${e.message}\"\n   133→                    )\n   134→                }\n   135→            }\n   136→        }\n   137→    }\n   138→\n   139→    /**\n   140→     * Loads the list of learned apps for display in the UI.\n   141→     *\n   142→     * Fetches all scraped apps from the database and converts them\n   143→     * to LearnedAppSummary objects with resolved app names.\n   144→     */\n   145→    fun loadLearnedApps() {\n   146→        viewModelScope.launch {\n   147→            try {\n   148→                val scrapedApps = databaseManager.scrapedApps.getAll()\n   149→\n   150→                val learnedApps = scrapedApps.map { app ->\n   151→                    // Get screen count for this app\n   152→                    val screenCount = databaseManager.screenContexts.countByApp(app.appId)\n   153→\n   154→                    // Resolve app name from package manager or use package name\n   155→                    val appName = resolveAppName(app.packageName)\n   156→\n   157→                    LearnedAppSummary(\n   158→                        packageName = app.packageName,\n   159→                        appName = appName,\n   160→                        screensLearned = screenCount.toInt(),\n   161→                        commandsGenerated = app.commandCount.toInt(),\n   162→                        lastLearnedTimestamp = app.lastScrapedAt\n   163→                    )\n   164→                }.sortedByDescending { it.lastLearnedTimestamp }\n   165→\n   166→                _uiState.update { it.copy(learnedApps = learnedApps) }\n   167→            } catch (e: Ex