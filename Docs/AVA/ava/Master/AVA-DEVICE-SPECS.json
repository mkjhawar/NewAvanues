  "compilation_requirements": {
    "toolchain": {
      "description": "AVA uses separate TVM versions for compilation and runtime",
      "compilation_tvm": "0.18.dev0 (bundled with mlc-ai-nightly 0.15)",
      "runtime_tvm": "0.22.0 (custom Android build)",
      "install": "python3 -m pip install --pre mlc-ai-nightly",
      "verify": "python3 -m mlc_llm --help",
      "supported_model_types": ["llama", "gemma2", "gemma3", "qwen2", "mistral", "phi3"],
      "unsupported_model_types": ["qwen3 (use pre-compiled Adreno models)"]
    },
    "adreno_gpu": {
      "devices": ["realwear-hmt1", "realwear-arc3", "realwear-navigator500", "realwear-navigator520", "realwear-navigatorz1", "vuzix-m400", "vuzix-m4000", "samsung-galaxy-s23", "samsung-galaxy-s24", "samsung-galaxy-s24-ultra", "motorola-razr-plus-2024", "motorola-edge-50-ultra"],
      "mlc_target": "android",
      "compilation": "Pre-compiled MLC models work across all Adreno devices",
      "same_binary": true,
      "model_directory": "ava-ai-models-external/llm/",
      "notes": "Use pre-compiled models from HuggingFace mlc-ai org"
    },
    "mali_gpu": {
      "devices": ["rokid-xcraft"],
      "mlc_target": "android-mali",
      "compilation": "Requires separate Mali OpenCL compilation",
      "same_binary": false,
      "model_directory": "ava-ai-models-external/llm-mali/",
      "compile_command": "python3 -m mlc_llm compile MODEL --model-type TYPE --device opencl --host aarch64-linux-android -o OUTPUT.tar",
      "compiled_models": ["AVA-LL32-1B16-mali.tar", "AVA-GE2-2B16-mali.tar"],
      "notes": "Model weights (.bin) are shared with Adreno, only .tar differs"
    },
    "apple_neural_engine": {
      "devices": ["apple-iphone-15-pro", "apple-iphone-15-pro-max", "apple-iphone-16-pro", "apple-iphone-16-pro-max"],
      "mlc_target": "ios",
      "compilation": "Requires iOS Metal compilation via mlc-llm",
      "same_binary": false,
      "model_directory": "ava-ai-models-external/llm-coreml/",
      "compile_command": "python3 -m mlc_llm compile MODEL --model-type TYPE --host arm64-apple-ios -o OUTPUT.tar",
      "compiled_models": ["AVA-LL32-1B16-ios.tar", "AVA-GE2-2B16-ios.tar"],
      "tools": ["mlc-ai-nightly", "Xcode"],
      "notes": "Include compiled .tar in Xcode project resources"
    },
    "documentation": "See Developer-Manual-Chapter54-Cross-GPU-Model-Compilation.md"
